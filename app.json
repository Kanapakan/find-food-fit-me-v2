{
  "expo": {
    "name": "find-food-fit-me-v2",
    "slug": "find-food-fit-me-v2",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "googleServicesFile": "./GoogleService-Info.plist",
      "supportsTablet": true,
      "infoPlist": {
        "NSMotionUsageDescription": "Needs Permission for Steps"
      },
      "bundleIdentifier": "com.kapi.findfoodv2"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.kapi.findfoodv2"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "./node_modules/expo-sample-pedometer/expo-sample-pedometer-plugin.js",
      "@react-native-firebase/app",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "5c59648f-06d7-4942-8596-9b835abc55aa"
      }
    }
  }
}
